 pipeline {

            agent any

              stages {
              
    stage ('terraform_init') {

    steps {
    cmd_exec('terraform init -input=false')
    

    }

    }
    stage ('terraform_Action') {
    steps {
    
     cmd_exec('terraform plan -input=false -out tfplan')

     cmd_exec('terraform apply -input=false tfplan')
    
    

    }

    }

    }

    }
